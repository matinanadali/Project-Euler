procedure main()
    limit := 1000
    sum := 0

    # Iterate over numbers from 1 to limit-1
    every i := 1 to limit - 1 do {
        # Check if i is a multiple of 3 or 5
        if (i % 3 = 0 | i % 5 = 0) then
            sum +:= i
    }

    # Print the result
    write("Sum of multiples of 3 or 5 below ", limit, " is: ", sum)
end
